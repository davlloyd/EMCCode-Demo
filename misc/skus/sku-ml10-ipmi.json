{
    "name": "Default IPMI settings for ML10V2 servers",
    "discoveryGraphName": "Graph.Obm.Ipmi.CreateSettings",
    "discoveryGraphOptions": {
        "defaults": {
            "user": "administrator",
            "password": "password"
        }
    },
    "rules": [
        {
            "path": "bmc.IP Address"
        },
        {
            "path": "dmi.Base Board Information.Manufacturer",
            "equals": "HP"
        }
    ]
}